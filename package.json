{
  "name": "prosemirror-math",
  "type": "module",
  "version": "0.2.2",
  "packageManager": "pnpm@10.30.3",
  "description": "Rendering math expressions in ProseMirror",
  "author": "ocavue <ocavue@gmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/ocavue",
  "homepage": "https://github.com/ocavue/prosemirror-math#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/ocavue/prosemirror-math.git"
  },
  "bugs": "https://github.com/ocavue/prosemirror-math/issues",
  "keywords": [
    "ProseMirror"
  ],
  "sideEffects": false,
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "lint": "eslint .",
    "fix": "eslint --fix . && prettier --write .",
    "test:install": "playwright install chromium",
    "test": "vitest",
    "typecheck": "tsc -b"
  },
  "dependencies": {
    "@ocavue/utils": "^1.5.0",
    "prosemirror-enter-rules": "^0.1.5",
    "prosemirror-inputrules": "^1.5.1",
    "prosemirror-model": "^1.25.4",
    "prosemirror-state": "^1.4.4",
    "prosemirror-view": "^1.41.6"
  },
  "devDependencies": {
    "@mathjax/src": "^4.1.1",
    "@ocavue/eslint-config": "^4.2.0",
    "@ocavue/tsconfig": "^0.6.3",
    "@prosekit/core": "^0.10.0",
    "@types/node": "^20.19.10",
    "@vitest/browser": "^4.0.18",
    "@vitest/browser-playwright": "^4.0.18",
    "diffable-html-snapshot": "^0.2.0",
    "eslint": "^10.0.2",
    "katex": "^0.16.33",
    "playwright": "^1.58.2",
    "prettier": "^3.8.1",
    "temml": "^0.13.1",
    "tsdown": "^0.20.3",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vitest": "^4.0.18",
    "vitest-browser-commands": "^0.2.0"
  }
}
